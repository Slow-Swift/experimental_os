    "version": "0.2.0",
    "configurations": [
        {
            "name": "OS Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/i686_debug/kernel/kernel.elf",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "logging": {
                "engineLogging": true
            },
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Run script",
                    "text": "source ${workspaceFolder}/.vscode/.gdb_script.gdb",
                    "ignoreFailures": false
                }
            ],
            "launchCompleteCommand": "None"
        }
    ]
}
